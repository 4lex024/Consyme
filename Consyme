 --ESP Medkit
local ESPConnection = nil
local ESPEnabled = false

local function AddESP(part)
    if part:FindFirstChild("ESP") then return end

    local adorn = Instance.new("BoxHandleAdornment")
    adorn.Name = "ESP"
    adorn.Adornee = part
    adorn.Parent = part
    adorn.Size = part.Size
    adorn.ZIndex = 0
    adorn.AlwaysOnTop = true
    adorn.Color3 = Color3.fromRGB(255, 105, 180) -- Warna Merah Muda (Pink)
    adorn.Transparency = 0.5 -- Transparansi kotak ESP
end

local Toggle = ItemTab:CreateToggle({
    Name = "ESP Medkit",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        ESPEnabled = Value

        if ESPEnabled then
            ESPConnection = game:GetService("RunService").RenderStepped:Connect(function()
                for _, v in pairs(workspace:GetChildren()) do
                    if v:IsA("BasePart") and v.Name == "Medkit" then
                        AddESP(v)
                    end
                end
            end)
        else
            if ESPConnection then
                ESPConnection:Disconnect()
                ESPConnection = nil
            end

            for _, v in pairs(workspace:GetChildren()) do
                if v:IsA("BasePart") and v:FindFirstChild("ESP") then
                    v.ESP:Destroy()
                end
            end
        end
    end,
})

--ESP Ayam
local ESPChicken = nil
local ESPEnabled = false

local function AddESP(target)
    if target:FindFirstChild("HighlightESP") then return end

    local highlight = Instance.new("Highlight")
    highlight.Name = "HighlightESP"
    highlight.FillColor = Color3.fromRGB(255, 255, 0) -- Warna Kuning
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- Warna Outline Putih
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Adornee = target
    highlight.Parent = target
end

local Toggle = ItemTab:CreateToggle({
    Name = "ESP Chicken",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        ESPEnabled = Value

        if ESPEnabled then
            ESPChicken = game:GetService("RunService").RenderStepped:Connect(function()
                local chickens = workspace:FindFirstChild("Chickens")
                if chickens then
                    local models = chickens:FindFirstChild("ChickenModels")
                    if models then
                        local spawned = models:FindFirstChild("Spawned")
                        if spawned then
                            for _, chicken in pairs(spawned:GetChildren()) do
                                if chicken:IsA("Model") and chicken:FindFirstChild("HumanoidRootPart") then
                                    AddESP(chicken)
                                end
                            end
                        end
                    end
                end
            end)
        else
            if ESPChicken then
                ESPChicken:Disconnect()
                ESPChicken = nil
            end

            local chickens = workspace:FindFirstChild("Chickens")
            if chickens then
                local models = chickens:FindFirstChild("ChickenModels")
                if models then
                    local spawned = models:FindFirstChild("Spawned")
                    if spawned then
                        for _, chicken in pairs(spawned:GetChildren()) do
                            if chicken:FindFirstChild("HighlightESP") then
                                chicken.HighlightESP:Destroy()
                            end
                        end
                    end
                end
            end
        end
    end,
})

local EnemyTab = Window:CreateTab("‚ò†Ô∏è ENEMY", nil) -- Title, Image
local MainSection = EnemyTab:CreateSection("Main")

--ESP Boss
local ESPBoss = nil
local ESPEnabled = false

local function AddESP(target)
    if target:FindFirstChild("HighlightESP") then return end

    local highlight = Instance.new("Highlight")
    highlight.Name = "HighlightESP"
    highlight.FillColor = Color3.fromRGB(255, 0, 0) -- Warna Merah
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- Warna Putih
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Adornee = target
    highlight.Parent = target
end

local Toggle = EnemyTab:CreateToggle({
    Name = "ESP Boss",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        ESPEnabled = Value

        if ESPEnabled then
            ESPBoss = game:GetService("RunService").RenderStepped:Connect(function()
                local boss = workspace:FindFirstChild("Boss")
                if boss then
                    local exports = boss:FindFirstChild("BossExports")
                    if exports then
                        local model = exports:FindFirstChild("MainSmlik")
                        if model then
                            AddESP(model)
                        end
                    end
                end
            end)
        else
            if ESPBoss then
                ESPBoss:Disconnect()
                ESPBoss = nil
            end

            local boss = workspace:FindFirstChild("Boss")
            if boss then
                local exports = boss:FindFirstChild("BossExports")
                if exports then
                    local model = exports:FindFirstChild("MainSmlik")
                    if model and model:FindFirstChild("HighlightESP") then
                        model.HighlightESP:Destroy()
                    end
                end
            end
        end
    end,
})

--ESP Crawler
local ESPCrawler = nil
local ESPEnabled = false

local function AddESP(target)
    if target:FindFirstChild("HighlightESP") then return end

    local highlight = Instance.new("Highlight")
    highlight.Name = "HighlightESP"
    highlight.FillColor = Color3.fromRGB(255, 0, 0) -- Warna Merah
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255) -- Outline Putih
    highlight.FillTransparency = 0.5
    highlight.OutlineTransparency = 0
    highlight.Adornee = target
    highlight.Parent = target
end

local Toggle = EnemyTab:CreateToggle({
    Name = "ESP Crawlers",
    CurrentValue = false,
    Flag = "ToggleCrawlerESP",
    Callback = function(Value)
        ESPEnabled = Value

        if ESPEnabled then
            ESPCrawler = game:GetService("RunService").RenderStepped:Connect(function()
                local crawlers = workspace:FindFirstChild("Crawlers")
                if crawlers then
                    for _, crawler in pairs(crawlers:GetChildren()) do
                        if crawler:IsA("Model") and crawler:FindFirstChild("HumanoidRootPart") then
                            AddESP(crawler)
                        end
                    end
                end
            end)
        else
            if ESPCrawler then
                ESPCrawler:Disconnect()
                ESPCrawler = nil
            end

            local crawlers = workspace:FindFirstChild("Crawlers")
            if crawlers then
                for _, crawler in pairs(crawlers:GetChildren()) do
                    if crawler:FindFirstChild("HighlightESP") then
                        crawler.HighlightESP:Destroy()
                    end
                end
            end
        end
    end,
})
